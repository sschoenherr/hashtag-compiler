begin

{PRIMERA CLASE}
 public class Hashtag
     begin
        String mensaje= "Hola MUNDO!!";
        print(mensaje);
         
       end
      
public double numeros () begin 
int opcion=0; 
int resultado=0;
int a,b=10;
       do 
      begin 
        switch (opcion) 
       begin
            case 1: 
                resultado = a + b;
                 break;
            case 2:
                System.out.println("mad");
        end
      endwhile while(opcion!=3);

end

   {CICLO FOR}
    for (int i = 0; i < 10; i++) begin
            print("i = " + i);
        endfor

end
